#include <iostream>
using namespace std;
int greatest(int,int,int);
int main() {
    cout<<"Enter Three Numbers : ";
    int a,b,c,g;
    cin>>a>>b>>c;
    g = greatest(a,b,c);
    cout<<"Greatest Number is : "<<g<<endl;
    
    return 0;
}
int greatest(int x, int y, int z)
{
    if(x > y && x > z)
    return x;
    else if(y > z && y > z)
    return y;
    else
    return z;
}

